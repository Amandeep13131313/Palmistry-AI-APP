{
  "name": "Sample PWA App",
  "short_name": "PWA App",
  "start_url": "/index.html",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#000000",
  "orientation": "portrait",
  "id": "/",
  "icons": [
    {
      "src": "icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "launch_handler": {
    "client_mode": "auto"
  },
  "scope": "/",
  "scope_extensions": [
    {
      "origin": "/"
    }
  ],
  "prefer_related_applications": false
}